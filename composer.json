{
  "name": "vrbata/xml-validator",
  "description": "Validates your XML against a xsd schema.",
  "keywords": ["XML", "XSD", "XML Validator", "psr-4"],
  "homepage": "http://github.com/vrbata/XmlValidator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josef Vrbata"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {"XmlValidator\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"XmlValidator\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}